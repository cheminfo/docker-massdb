  },
  "homepage": "https://github.com/cheminfo/docker-massdb#readme",
  "dependencies": {
    "array-xy-max-y": "^0.1.0",
    "buffer-split": "^1.0.0",
    "convert-to-jcamp": "^1.1.3",
    "debug": "^4.1.0",
    "delay": "^4.1.0",
    "fs-extra": "^6.0.1",
    "jcampconverter": "^2.10.2",
    "json-to-html": "^0.1.2",
    "kcors": "^2.2.2",
    "koa": "^2.5.0",
    "koa-router": "^7.4.0",
