              ],
              "script": [
                "let action=this.action.name;\nlet value=this.action.value;\n\n\nconst parse=(await API.require('https://www.lactame.com//lib/xy-parser/2.2.2/xy-parser.js')).parseXY;\nlet spectrum=parse(value.mass,{arrayType:'xxyy'});\n\nconsole.log(action);\n\nswitch (action) {\n    case 'changed':\n        change(value, spectrum);\n        break;\n    case 'submitted':\n        submit(value, spectrum);\n        break;\n}\n\nasync function change(value, spectrum) {\n    // we recreate the experimental spectrum\n    API.createData('experimental', {\n        x:spectrum[0],\n        y:spectrum[1]\n    });\n}\n\nasync function submit(value, spectrum) {\n    let idCode=String(API.getData('idCode').value);\n    console.log(idCode);\n    const params=[];\n    if (value.minMass) params.push('minMass='+encodeURIComponent(value.minMass));\n    if (value.maxMass) params.push('maxMass='+encodeURIComponent(value.maxMass));\n    if (value.mf) params.push('mf='+encodeURIComponent(value.mf));\n    if (value.limit) params.push('limit='+encodeURIComponent(value.limit));\n    if (value.minSimilarity) params.push('minSimilarity='+encodeURIComponent(value.minSimilarity));\n\n    if (spectrum[0].length>0) {\n        params.push('mz='+encodeURIComponent(spectrum[0].join(',')));\n        params.push('intensity='+encodeURIComponent(spectrum[0].join(',')));\n    }\n\n    if (idCode) params.push('idCode='+encodeURIComponent(idCode));\n    \n    let url='/search/mass?'+params.join('&');\n\n    \n    UI.progress('searching', 'Searching ...');\n    let response=await fetch(url);\n    let results=await response.json();\n    UI.stopProgress('searching');\n    API.createData('results', results);\n}\n\n\n"
              ]
            }
